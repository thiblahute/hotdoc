{
    "output_format": "html", 
    "git_repo": "..", 
    "index": "markdown_files/index.markdown",
    "python_sources": ["../hotdoc/core/*.py"],
    "python_source_filters": ["../hotdoc/core/__init__.py"],
    "python_smart_index": true,
    "add_anchors": true,
    "output": "built_doc",
    "project_name": "hotdoc",
    "project_version": "0.X",
    "sitemap": "sitemap.txt",
    "syntax_highlighting_activate": true,
    "devhelp_activate": true,
    "default-license": "CC-BY-SAv4.0",
    "git_upload_repository": "git@github.com:hotdoc/hotdoc.github.io.git"
}
